{
    "name": "vigihdev/dto",
    "description": "Data Transfer Objects (DTO) library for Vigihdev ecosystem. Provides strict, immutable, and serializer-friendly DTO classes.",
    "type": "library",
    "keywords": [
        "dto",
        "data-transfer-object",
        "serializer",
        "vigihdev",
        "php",
        "symfony"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Vigih Dev",
            "email": "vigihdev@gmail.com",
            "role": "developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.id"
        }
    ],
    "require": {
        "php": "^8.1",
        "vigihdev/contracts": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Vigihdev\\Dto\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vigihdev\\Dto\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.x-dev",
        "vigihdev/serializer": "dev-main",
        "vigihdev/symfony-bridge-config": "^1.0"
    },
    "scripts": {
        "test": "phpunit",
        "server": "php -S localhost:2834 server.php"
    }
}
